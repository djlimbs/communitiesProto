{{#if isOnePage}}
    <div class="wizardSection pad--sm--only--n" data-qa-container="legallyRequiredOnePage">
        <h3 class="pad--sm--only--lm pad--sm--only--rm mar--md--bn mar--md--txl" data-qa-header="legallyRequired">{{labels.legallyRequired}}</h3>
        {{partial "formElements"}}
    </div>
{{else}}
    <div class="wizardSection pad--sm--only--n" data-qa-container="legallyRequired">
        <h1 class="text-left-center page__heading pad--sm--tm pad--md--tn pad--sm--bm" data-qa-header="legallyRequired">{{labels.legallyRequired}}</h1>
        {{#if errorMessage}}
            {{partial "errorAlert"}}
        {{/if}}
        {{partial "formElements"}}
    </div>
{{/if}}