{{#with Recommendations__r}}
    <section id="recommendations">
        <div class="list-heading">
            <h4 data-qa-label="recommendations" class="list-heading__title">{{../labels.recommendations}}</h4>
        </div>
        <div class="panel__body pad--sm--bn">
            {{#each records}}
                <article class="mar--sm--bm">
                    <div class="card mar--sm--tn">
                        <div class="card__body">
                            <p data-qa-label="recommendationText" class="mar--sm--n">
                                {{#if Recommendation_Text__c}}
                                    "{{convertNewLinesToBreaks Recommendation_Text__c}}"
                                {{/if}}
                            </p>
                            <div data-qa-label="recommendationTitle" class="text-right">
                                <p data-qa-label="recommendationName" class="mar--sm--bn">
                                    {{Name}}
                                </p>
                                {{Recommender_Title__c}}
                            </div>
                        </div>
                    </div>
                    <hr class="hr-alt"/>
                </article>
            {{/each}}
        </div>
    </section>
{{/with}}
{{#with Certifications__r}}
    <section id="certifications">
        <div class="list-heading">
            <h4 data-qa-label="certifications" class="list-heading__title">{{../labels.certifications}}</h4>
        </div>
        <div class="test panel__body pad--sm--bn">
            {{#each records}}
                <article class="mar--sm--bm">
                    <h3 class="mar--sm--n" data-qa-label="certificationName">{{Name}}</h3>
                    <small data-qa-label="certificationIssuer" class="text-faded block">{{Issuer__c}} 
                        {{#if Issuer__c}}
                            {{#if Number__c}}({{Number__c}}){{/if}}
                        {{else}}
                            {{Number__c}}
                        {{/if}}
                    </small>
                    <small data-qa-label="certificationDate" class="text-faded">{{displayMonthYearRange Start_Month__c Start_Year__c End_Month__c End_Year__c "to"}}</small>
                    <hr class="hr-alt"/>
                </article>
            {{/each}}
        </div>
    </section>
{{/with}}
{{#with Employment_Histories__r}}
    <section id="employment">
        <div class="list-heading">
            <h4 data-qa-label="employment" class="list-heading__title">{{../labels.employment}}</h4>
        </div>
        <div class="panel__body pad--sm--bn">
            {{#each records}}
                <article class="mar--sm--bm">
                    <h3 class="mar--sm--n" data-qa-label="employmentTitle">{{Job_Title__c}}</h3>
                    <small data-qa-label="employmentName" class="text-faded block">{{Name}}</small>
                    <small data-qa-label="employmentDate" class="text-faded">{{displayMonthYearRange Start_Month__c Start_Year__c End_Month__c End_Year__c "to"}}</small>
                    {{#if Description__c}}
                        <p data-qa-label="employmentDate" class="mar--sm--bn">{{convertNewLinesToBreaks Description__c}}</p>
                    {{/if}}
                    {{#if fieldSet}}
                        <div class="panel__body pad--sm--n">
                            <div class="row pad--sm--rxxl">
                                {{#each fieldSet}}
                                    <div class="column--sm--6 column--xs--only--12 pad--sm--only--tm mar--md--tm">
                                        <label class="block">{{label}}</label>
                                        {{#if value}}
                                            {{{value}}}
                                        {{else}}
                                            <div data-qa-label="notProvided" class="text-faded">{{../../../../../labels.notProvided}}</div>
                                        {{/if}}
                                    </div>
                                {{/each}}
                            </div>
                        </div>
                    {{/if}}
                    <hr class="hr-alt"/>
                </article>
            {{/each}}
        </div>
    </section>
{{/with}}
