{{#with Patents__r}}
    <section id="patents">
        <div class="list-heading">
            <h4 data-qa-label="patents" class="list-heading__title">{{../labels.patents}}</h4>
        </div>
        <div class="panel__body pad--sm--bn">
            {{#each records}}
                <article class="mar--sm--bm">
                    <h3 class="mar--sm--n" data-qa-label="patentName">
                        {{#if Link__c}}
                            <a data-qa-link="patentNameWithLink" {{action "navigation" Link__c}} href="#" target>{{Name}}</a>
                        {{else}}
                            {{Name}}
                        {{/if}}
                    </h3>
                    <small data-qa-label="patentNumberAndOffice" class="text-faded block">
                        {{#if Number__c}}
                            {{Number__c}} {{#if Office__c}}({{Office__c}}){{/if}}
                        {{else}}
                            {{Office__c}}
                        {{/if}}
                    </small>
                    <small data-qa-label="patentDateAndStatus" class="text-faded">
                        {{#if Date__c}}
