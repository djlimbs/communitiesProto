                        {{else}}
                            {{Status__c}}
                        {{/if}}
                    </small>
                    <p data-qa-label="patentSummary">
                        {{convertNewLinesToBreaks Summary__c}}
                    </p>
                    <hr class="hr-alt"/>
                </article>
            {{/each}}
        </div>
    </section>
