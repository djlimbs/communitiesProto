{{#if isOnePage}}
    <div id="general" class="wizardSection pad--sm--only--n" data-qa-container="generalOnePage">
        <h3 class="pad--sm--only--lm pad--sm--only--rm mar--md--bn mar--md--txl" data-qa-header="general">{{labels.general}}</h3>
        {{partial "formElements"}}
    </div>
{{else}}
    <div class="wizardSection pad--sm--only--n" data-qa-container="general">
        <h1 class="text-left-center page__heading pad--sm--tm pad--md--tn pad--sm--bm" data-qa-label="general">{{labels.general}}</h1>
        {{#if errorMessage}}
            {{partial "errorAlert"}}
        {{/if}}
        {{partial "formElements"}}
    </div>
{{/if}}