{{#with tp.Projects__r as projects}}
    <div class="slds-card slds-overflow--hidden">
        <div class="slds-card__header">
            <h3 class="slds-text-heading--label slds-truncate">{{labels.projects}}</h3>
        </div>
        {{#each projects.records}}
            <div class="slds-card__body slds-p-around--small">
                <div class="slds-card slds-theme--default slds-p-around--medium slds-text-longform">
                    <h4 class="slds-text-heading--small slds-truncate">{{Name}}</h4>
                    <ul class="slds-list--horizontal slds-has-dividers slds-tile__meta slds-text-body--small">
                        <li class="slds-list__item">{{displayDate Start_Date__c End_Date__c "to"}}</li>
                    </ul>
                    <p>{{convertNewLinesToBreaks Description__c}}</p>
                </div>
            </div>
        {{/each}}
    </div>
{{/with}}
{{#with tp.Recognition__r as recognition}}
    <div class="slds-card slds-overflow--hidden">
        <div class="slds-card__header">
            <h3 class="slds-text-heading--label slds-truncate">{{labels.recognition}}</h3>
        </div>
        <div class="slds-card__body slds-p-around--small">
            {{#each recognition.records itemController="additionalInfo"}}
                <div class="slds-card slds-theme--default slds-p-around--medium">
                    <h4 class="slds-text-heading--small slds-truncate" data-qa-label="recognitionName">
                        {{#if camelizedModel.link}}
                            <a data-qa-link="recognitionNameWithLink" {{action "navigation" Link__c}} href="#" target>{{Name}}</a>
                        {{else}}
                            {{Name}}
                        {{/if}}
                    </h4>
                    <p class="slds-text-body--small slds-text--disabled">{{Issuer__c}}</p>
                    <p class="slds-text-body--small slds-text--disabled">{{formatDate Date__c}}</p>
                    <div class="slds-m-top--medium slds-text-longform">
                        <p>{{convertNewLinesToBreaks Description__c}}</p>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
{{/with}}
