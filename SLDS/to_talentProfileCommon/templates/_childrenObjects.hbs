{{#with tp.Employment_Histories__r as empHists}}
    <div class="slds-card slds-overflow--hidden">
        <div class="slds-card__header">
            <h3 class="slds-text-heading--label slds-truncate" data-qa-label="employment">{{labels.employment}}</h3>
        </div>
        <div class="slds-card__body slds-p-around--small">
            {{#each empHists.records itemController="additionalInfo"}}
                <!-- Card -->
                <div class="slds-card slds-theme--default slds-p-around--medium">
                    <h4 class="slds-text-heading--small slds-truncate" data-qa-label="employmentTitle">{{Job_Title__c}}</h4>
                    <p class="slds-text-body--small slds-text--disabled" data-qa-label="employmentName">{{Name}}</p>
                    <p class="slds-text-body--small slds-text--disabled" data-qa-label="employmentDate">{{displayMonthYearRange Start_Month__c Start_Year__c End_Month__c End_Year__c "to"}}</p>
                    {{#if camelizedModel.description}}
                        <div class="slds-m-top--medium slds-text-longform">
                            <p data-qa-label="employmentDescription">{{convertNewLinesToBreaks Description__c}}</p>
                        </div>
                    {{/if}}
                    {{#if fieldSet}}
                        <hr class="slds-m-top--small slds-m-bottom--small">
                        <div class="slds-grid">
                            <!-- Field -->
                            {{#each fieldSet}}
                                <div class="slds-col ">
                                    <p class="slds-text-body--small">{{label}}</p>
                                    <p>
                                        {{#if value}}
                                            {{{value}}}
                                        {{else}}
                                            <span data-qa-label="notProvided">{{labels.notProvided}}</span>
                                        {{/if}}
                                    </p>
                                </div>
                            {{/each}}
                        </div>
                    {{/if}}
                </div>
            {{/each}}
        </div>
    </div>
{{/with}}
