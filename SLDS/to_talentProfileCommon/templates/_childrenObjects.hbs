{{#with tp.Projects__r as projects}}
    <section id="projects">
        <div class="list-heading">
            <h4 data-qa-label="projects" class="list-heading__title">{{labels.projects}}</h4>
        </div>
        <div class="panel__body pad--sm--bn">
            {{#each projects.records}}
                <article class="mar--sm--bm">
                    <h3 class="mar--sm--n" data-qa-label="projectName">{{Name}}</h3>
                    <small data-qa-label="projectDate" class="text-faded">{{displayDate Start_Date__c End_Date__c "to"}}</small>
                    <p data-qa-label="projectDescription">
                        {{convertNewLinesToBreaks Description__c}}
                    </p>
                    <hr class="hr-alt"/>
                </article>
            {{/each}}
        </div>
    </section>
{{/with}}
{{#with tp.Recognition__r as recognition}}
    <section id="recognition">
        <div class="list-heading">
            <h4 data-qa-recognition="recognition" class="list-heading__title">{{labels.recognition}}</h4>
        </div>
        <div class="panel__body pad--sm--bn">
            {{#each recognition.records itemController="additionalInfo"}}
                <article class="mar--sm--bm">
                    <h3 class="mar--sm--n" data-qa-label="recognitionName">
                        {{#if camelizedModel.link}}
                            <a data-qa-link="recognitionNameWithLink" {{action "navigation" Link__c}} href="#" target>{{Name}}</a>
                        {{else}}
                            {{Name}}
                        {{/if}}
                    </h3>
                    <small data-qa-label="recognitionIssuer" class="text-faded block">{{Issuer__c}}</small>
                    <small data-qa-label="recognitionDate" class="text-faded">{{displayMonthYear Month__c Year__c}}</small>
                    <p data-qa-label="recognitionDescription">
                        {{convertNewLinesToBreaks Description__c}}
                    </p>
                    <hr class="hr-alt"/>
                </article>
            {{/each}}
        </div>
    </section>
{{/with}}
